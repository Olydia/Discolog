         <step name="restaurantchoice" task="ChooseRestaurant"/>
         <step name="invitationrestau" task="InviteToRestaurant"/>
         <step name="booking" task="BookTable"/>
         <step name="rdv" task="RDV"/>
      </subtasks>
   </task>
   <task id="DialogueIntro">
      <subtasks id="_dominant">
         <step name="dominvit" task="DominantInvitation"/>
         <applicable> $AGENTRELATION==Relation.ENUM.dominant </applicable>
      </subtasks>
      <subtasks id="_peer">
         <step name="peerinvit" task="PeerInvitation"/>
         <applicable> $AGENTRELATION==Relation.ENUM.peer </applicable>
      </subtasks>
      <subtasks id="_sub">
         <step name="subinvit" task="SubmInvitation"/>
         <applicable> $AGENTRELATION==Relation.ENUM.submissive </applicable>
      </subtasks>
   </task>
   <task id="BookTable">
      <subtasks id="dominantbook">
         <step name="dominbook" task="Booking"/>
         <applicable> $AGENTRELATION==Relation.ENUM.dominant || $AGENTRELATION==Relation.ENUM.peer</applicable>
      </subtasks>
      <subtasks id="subbooking">
         <step name="subbook" task="SubmBooking"/>
         <applicable> $AGENTRELATION==Relation.ENUM.submissive </applicable>
      </subtasks>
   </task>
   <task id="ChooseRestaurant">
      <subtasks id="japaneserestau">
         <step name="japanesefood" task="ChooseJapaneseRestaurant"/>
         <applicable> $RESTOFOOD==listPreferences[0] || $RESTOFOOD== Preference.ENUM.japanese </applicable>
      </subtasks>
      <subtasks id="italianrestau">
         <step name="italianfood" task="ChooseItalianRestaurant"/>
         <applicable> $RESTOFOOD==listPreferences[1] || $RESTOFOOD== Preference.ENUM.italian </applicable>
      </subtasks>
   </task>
   <task id="SubmInvitation">
      <subtasks id="invitesub">
         <step name="ask" task="UserAvailable"/>
